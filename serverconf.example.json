{
    "env":"dev",
    "repository_name":"myrepo",
    "default_mountbase":"/tmp/dockerstatic/",
    "nginx_dyn_conf_dir":"/etc/nginx/dynamic-conf",
    "volumes":[
        {
            "tag":"static_files",
            "host":"default",
            "cont":"/srv/service/api/static/",
            "auto_clean":true
        }
    ],
    "server_map":[
        {
            "c_port":"8000/tcp",
            "v_host":"localhost",
            "volumes_served":[
                {
                    "tag":"static_files",
                    "location":"/static/"
                }
            ]
        }
    ]
}
